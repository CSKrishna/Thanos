{
    "model_version": "THANOS",
    "bidirectional": false,
    "GRU_num_units": 300,
    "embedding_size": 300,  
    "l2_reg": false,
    "l2_lambda": 1e-8,       

    "learning_rate":  9e-4,
    "batch_size": 32,
